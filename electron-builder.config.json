{
  "productName":"星云音乐",
  "copyright":"Shanghai Huisheng Internet Technology Co., Ltd.",
  "directories": {
    "output": "build"
  },
  "extraResources":[
    {
      "from":"./public/db/",
      "to": "db"
    },
    {
      "from":"./public/images/",
      "to": "images"
    }
  ],
  "nsis": {
    "oneClick": false,
    "perMachine": true,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "./public/images/logo.ico",
    "uninstallerIcon": "./public/images/logo.ico",
    "installerHeaderIcon": "./public/images/logo.ico",
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true,
    "shortcutName": "星云音乐"
  },

  "win": {
    "icon": "./public/images/logo.ico",
    "requestedExecutionLevel": "requireAdministrator"
  },

  "linux": {
    "icon": "./public/images/logo.ico"
  },

  "mac": {
    "icon": "./public/images/logo.ico"
  },
  "electronDownload": {
    "mirror": "https://mirrors.huaweicloud.com/electron/"
  }
}